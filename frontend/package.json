{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "export NODE_ENV=development && dotenv -c -- next dev",
    "build": "export NODE_ENV=production && next build",
    "start": "dotenv -c -- next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{css,scss,jsx,tsx,ts}\"",
    "fakeserver": "yarn json-server --watch ./src/pages/api/mock.json --port 3333",
    "docker:start": "docker compose up -d",
    "docker:stop": "docker compose down"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@next/font": "13.1.6",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/node": "18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "axios": "^1.3.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint": "8.33.0",
    "json-server": "^0.17.1",
    "lucide-react": "^0.316.0",
    "next": "13.1.6",
    "next-images": "1.8.4",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.3",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.4.1",
    "dotenv-cli": "^7.3.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.33",
    "prettier": "2.8.3",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "typescript": "4.9.5"
  }
